{
  "name": "dinnerplano",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.5.6",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.3",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.3.0",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "BROWSER=none REACT_APP_COMMIT_HASH=\"$(npm run -s commit-hash)\" react-scripts start",
    "build": "REACT_APP_COMMIT_HASH=\"$(npm run -s commit-hash)\" react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,jsx --fix .",
    "commit-hash": "git rev-parse HEAD | xxd -r -p | base64 | tr -d '='",
    "commit-hash-rev": "base64 -D | xxd -p",
    "deploy": "npm run build && firebase deploy"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "firebase-tools": "^6.0.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.6.0",
    "redux-mock-store": "^1.5.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
